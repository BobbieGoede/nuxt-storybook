{
    "name": "@storybook-vue/nuxt-storybook",
    "version": "0.0.1-beta.0",
    "description": "Storybook module for Nuxt",
    "license": "MIT",
    "repository": "nuxt-modules/storybook",
    "packageManager": "pnpm@8.7.0",
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist",
        "*.d.ts",
        "*.cjs",
        "*.mjs"
    ],
    "scripts": {
        "build": "pnpm build:module ",
        "build:module": "nuxt-build-module",
        "dev": "nuxi dev playground",
        "typecheck": "nuxi typecheck",
        "prepack": "pnpm build",
        "prepare": "nuxi prepare playground"
    },
    "devDependencies": {
        "@iconify-json/carbon": "^1.1.20",
        "@nuxt/devtools": "0.8.2",
        "@nuxt/devtools-ui-kit": "0.8.2",
        "@nuxt/module-builder": "^0.4.0",
        "@nuxt/schema": "^3.7.0",
        "@nuxt/test-utils": "^3.7.0",
        "eslint": "^8.48.0",
        "nuxt": "^3.7.0"
    },
    "dependencies": {
        "@nuxt/devtools-kit": "0.8.2",
        "@nuxt/kit": "^3.7.0",
        "get-port-please": "^3.0.2",
        "sirv": "^2.0.3",
        "typescript": "^5.2.2",
        "wait-on": "^7.0.1",
        "vue-router": "^4.2.4"
    },
    "publishConfig": {
        "access": "public"
    }
}